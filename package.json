{
  "name": "cofinder",
  "displayName": "码小白 CoFinder",
  "description": "码小白 CoFinder",
  "version": "0.0.5",
  "icon": "media/logo.png",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "preview": true,
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "submenus": [
      {
        "id": "cofinder",
        "label": "🔍 码小白 CoFinder"
      }
    ],
    "commands": [
      {
        "command": "cofinder.reviewFile",
        "title": "CoReview: Review 整个文件"
      },
      {
        "command": "cofinder.reviewSeletion",
        "title": "CoReview: Review 选中代码"
      },
      {
        "command": "cofinder.rewriteFile",
        "title": "CoWrite: 重写整个文件"
      },
      {
        "command": "cofinder.rewriteSeletion",
        "title": "CoWrite: 重写选中代码"
      }
    ],
    "configuration": {
      "title": "CoFinder",
      "order": 0,
      "properties": {
        "cofinder.server": {
          "type": "string",
          "default": "https://cofinder.api.ixiqin.com/api/",
          "description": "CoFinder API 地址"
        }
      }
    },
    "menus": {
      "editor/context": [
        {
          "submenu": "cofinder",
          "group": "navigation"
        }
      ],
      "cofinder": [
        {
          "when": "editorHasSelection",
          "command": "cofinder.rewriteSeletion",
          "group": "rewrite"
        },
        {
          "when": "editorTextFocus && (!editorHasSelection)",
          "command": "cofinder.rewriteFile",
          "group": "rewrite"
        },
        {
          "when": "editorHasSelection",
          "command": "cofinder.reviewSeletion",
          "group": "review"
        },
        {
          "when": "editorTextFocus && (!editorHasSelection)",
          "command": "cofinder.reviewFile",
          "group": "review"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "yarn run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "fast-printf": "^1.6.9"
  },
  "repository": {
    "url": "https://github.com/Hackathon20230415/vscode/tree/main"
  },
  "publisher": "bestony"
}
